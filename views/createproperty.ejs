<%- include("partials/header")%>
  <div class="mt-10 w-[100%] mx-auto p-5">
    <h1 class="text-[3vw] p-4 text-center uppercase font-[700]">welcome to property creating page</h1> <br> 
    <form class="ml-[30vw]" action="/property" method="post" enctype="multipart/form-data">
      <input class="border-2 border-slate-900 p-2" type="text" name="title" placeholder="Title"> <br> <br>
      <input class="border-2 border-slate-900 p-2" type="text" name="description" placeholder="Description"> <br> <br>
      <input class="border-2 border-slate-900 p-2" type="number" name="price" placeholder="Price"><br> <br>
      <input class="border-2 border-slate-900 p-2" type="text" name="location" placeholder="Location"><br> <br>
      <input class="border-2 border-slate-900 p-2" type="file" name="image" placeholder="Image"><br> <br>
      <input class="border-2 border-slate-900 p-2" type="status" name="status" placeholder="available | sold | pending"><br> <br>
      <button class="px-[3vw] py-[.5vw] bg-green-400 rounded">Submit</button>
    </form>
         
  </div>
  <%- include("partials/footer")%>